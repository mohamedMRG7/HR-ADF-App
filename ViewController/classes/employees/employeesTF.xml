<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="employeesTF">
    <default-activity>employeesView</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="employeesView">
      <page>/employeesView.jsff</page>
    </view>
    <view id="addEmployee">
      <page>/addEmployee.jsff</page>
    </view>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Rollback">
      <method>#{bindings.Rollback.execute}</method>
      <outcome>
        <fixed-outcome>Rollback</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>employeesView</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>add</from-outcome>
        <to-activity-id>CreateInsert</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>addEmployee</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>addEmployee</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>back</from-outcome>
        <to-activity-id>Rollback</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>Rollback</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>Rollback</from-outcome>
        <to-activity-id>employeesView</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
