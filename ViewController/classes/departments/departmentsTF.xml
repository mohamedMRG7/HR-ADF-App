<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="departmentsTF">
    <default-activity>departmetsView</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="departmetsView">
      <page>/departmetsView.jsff</page>
    </view>
    <view id="addDepartment">
      <page>/addDepartment.jsff</page>
    </view>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Rollback">
      <method>#{bindings.Rollback.execute}</method>
      <outcome>
        <fixed-outcome>Rollback</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__7">
      <from-activity-id>departmetsView</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>add</from-outcome>
        <to-activity-id>CreateInsert</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>addDepartment</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__11">
      <from-activity-id>addDepartment</from-activity-id>
      <control-flow-case id="__12">
        <from-outcome>back</from-outcome>
        <to-activity-id>Rollback</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__13">
      <from-activity-id>Rollback</from-activity-id>
      <control-flow-case id="__14">
        <from-outcome>Rollback</from-outcome>
        <to-activity-id>departmetsView</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
